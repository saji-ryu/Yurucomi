{
  "name": "yurucomi-server",
  "description": "",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "DEBUG=server:*,nodemon:*,express:* NODE_ENV=development nodemon src/index.ts --exec babel-node --watch src --extensions '.ts'",
    "build": "npm run build:clean && npm run build:babel",
    "build:clean": "rm -rf dist && mkdir -p dist",
    "build:babel": "../../node_modules/.bin/babel src --out-dir dist --source-maps --extensions '.ts'",
    "type-check": "../../node_modules/.bin/tsc --noEmit",
    "test": "echo no test"
  },
  "devDependencies": {
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-typescript": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.4",
    "@types/debug": "^0.0.31",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/mongodb": "^3.1.12",
    "@types/socket.io": "^1.4.38",
    "lerna": "^2.11.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "debug": "^4.1.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "mongodb": "^3.1.8",
    "socket.io": "^2.1.1",
    "yurucomi-interfaces": "^0.0.1"
  }
}
