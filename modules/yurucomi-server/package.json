{
  "name": "yurucomi-server",
  "description": "",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "DEBUG=server:*,nodemon:* NODE_ENV=development nodemon src/server/index.ts --exec babel-node --watch src --watch public --extensions '.ts'",
    "build": "npm run build-server && npm run build-webpack",
    "build-server": "npm run build-server:clean && npm run build-server:babel",
    "build-server:clean": "rm -rf dist && mkdir -p dist",
    "build-server:babel": "../../node_modules/.bin/babel src/server --out-dir dist --source-maps --extensions '.ts'",
    "build-webpack": "webpack -p --progress --config webpack.config.js",
    "build-webpack-watch": "webpack -p --progress --watch --config webpack.config.js",
    "type-check": "../../node_modules/.bin/tsc --noEmit",
    "test": "echo no test"
  },
  "devDependencies": {
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-typescript": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.4",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "^0.0.31",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.11",
    "@types/eventemitter2": "^4.1.0",
    "@types/mongodb": "^3.1.12",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/socket.io": "^1.4.38",
    "@types/socket.io-client": "^1.4.32",
    "ts-loader": "^5.2.1",
    "lerna": "^2.11.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.1.3",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
